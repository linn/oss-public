    </specVersion>

    <actionList>
        <action>
            <name>Read</name>
            <argumentList>
                <argument>
                    <name>aId</name>
                    <direction>in</direction>
                    <relatedStateVariable>Id</relatedStateVariable>
                </argument>
                <argument>
                    <name>aUri</name>
                    <direction>out</direction>
                    <relatedStateVariable>Data</relatedStateVariable>
                </argument>
                <argument>
                    <name>aMetaData</name>
                    <direction>out</direction>
                    <relatedStateVariable>Data</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>ReadList</name>
            <argumentList>
                <argument>
                    <name>aIdList</name>
                    <direction>in</direction>
                    <relatedStateVariable>Data</relatedStateVariable>
                </argument>
                <argument>
                    <name>aMetaDataList</name>
                    <direction>out</direction>
                    <relatedStateVariable>Data</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
