                    <direction>in</direction>
                    <relatedStateVariable>Id</relatedStateVariable>
                </argument>
                <argument>
                    <name>aUri</name>
                    <direction>in</direction>
                    <relatedStateVariable>Data</relatedStateVariable>
                </argument>
                <argument>
                    <name>aMetaData</name>
                    <direction>in</direction>
                    <relatedStateVariable>Data</relatedStateVariable>
                </argument>
                <argument>
                    <name>aNewId</name>
                    <direction>out</direction>
                    <relatedStateVariable>Id</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>Delete</name>
            <argumentList>
                <argument>
                    <name>aId</name>
