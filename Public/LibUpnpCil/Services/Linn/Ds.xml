<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
    <specVersion>
        <major>1</major>
        <minor>0</minor>
    </specVersion>

    <actionList>
        <action>
            <name>Play</name>
        </action>
        <action>
            <name>Pause</name>
        </action>
        <action>
            <name>Stop</name>
        </action>
        <action>
            <name>SeekSecondAbsolute</name>
            <argumentList>
                <argument>
                    <name>aSecond</name>
                    <direction>in</direction>
                    <relatedStateVariable>Pos</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SeekSecondRelative</name>
            <argumentList>
                <argument>
                    <name>aSecond</name>
                    <direction>in</direction>
                    <relatedStateVariable>Offset</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SeekTrackId</name>
            <argumentList>
                <argument>
                    <name>aTrackId</name>
                    <direction>in</direction>
                    <relatedStateVariable>TrackId</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SeekTrackAbsolute</name>
            <argumentList>
                <argument>
                    <name>aTrack</name>
                    <direction>in</direction>
                    <relatedStateVariable>Pos</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SeekTrackRelative</name>
            <argumentList>
                <argument>
                    <name>aTrack</name>
                    <direction>in</direction>
                    <relatedStateVariable>Offset</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>State</name>
            <argumentList>
                <argument>
                    <name>aTransportState</name>
                    <direction>out</direction>
                    <relatedStateVariable>TransportState</relatedStateVariable>
                </argument>
                <argument>
                    <name>aTrackDuration</name>
                    <direction>out</direction>
                    <relatedStateVariable>TrackDuration</relatedStateVariable>
                </argument>
                <argument>
                    <name>aTrackBitRate</name>
                    <direction>out</direction>
                    <relatedStateVariable>TrackBitRate</relatedStateVariable>
                </argument>
                <argument>
                    <name>aTrackLossless</name>
                    <direction>out</direction>
                    <relatedStateVariable>TrackLossless</relatedStateVariable>
                </argument>
                <argument>
                    <name>aTrackBitDepth</name>
                    <direction>out</direction>
                    <relatedStateVariable>TrackBitDepth</relatedStateVariable>
                </argument>
                <argument>
                    <name>aTrackSampleRate</name>
                    <direction>out</direction>
                    <relatedStateVariable>TrackSampleRate</relatedStateVariable>
                </argument>
                <argument>
                    <name>aTrackCodecName</name>
                    <direction>out</direction>
                    <relatedStateVariable>TrackCodecName</relatedStateVariable>
                </argument>
                <argument>
                    <name>aTrackId</name>
                    <direction>out</direction>
                    <relatedStateVariable>TrackId</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>ProtocolInfo</name>
            <argumentList>
                <argument>
                    <name>aSupportedProtocols</name>
                    <direction>out</direction>
                    <relatedStateVariable>SupportedProtocols</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
    </actionList>

    <serviceStateTable>
        <stateVariable sendEvents="no">
            <name>Offset</name>
            <dataType>i4</dataType>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>Pos</name>
            <dataType>ui4</dataType>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>SupportedProtocols</name>
            <dataType>string</dataType>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>TrackDuration</name>
            <dataType>ui4</dataType>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>TrackBitRate</name>
            <dataType>ui4</dataType>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>TrackLossless</name>
            <dataType>boolean</dataType>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>TrackBitDepth</name>
            <dataType>ui4</dataType>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>TrackSampleRate</name>
            <dataType>ui4</dataType>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>TrackCodecName</name>
            <dataType>string</dataType>
        </stateVariable>
        <stateVariable sendEvents="yes">
          <name>TrackId</name>
          <dataType>ui4</dataType>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>TransportState</name>
            <dataType>string</dataType>
            <allowedValueList>
                <allowedValue>Playing</allowedValue>
                <allowedValue>Paused</allowedValue>
                <allowedValue>Stopped</allowedValue>
                <allowedValue>Buffering</allowedValue>
            </allowedValueList>
        </stateVariable>
    </serviceStateTable>
</scpd>

